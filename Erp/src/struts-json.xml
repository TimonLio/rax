<struts>

	<package name="json" extends="json-default">
		<action name="ware_find" class="WareFindAction">
			<result type="json" />
		</action>
		<action name="ware_find_ft" class="WareFindAction" method="search">
			<result type="json" />
		</action>
		<action name="sell_item_add" class="SellItemAction">
			<result type="json" />
		</action>
		<action name="order_item_add" class="OrderItemAction">
			<result type="json" />
		</action>
		<action name="sell_item_import" class="SellItemImportAction">
			<result type="json" />
		</action>
	</package>

</struts>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="json" extends="json-default">
		<action name="ware_find" class="WareFindAction">
			<result type="json" />
		</action>
		<action name="ware_find_fulltext" class="WareFindAction"
			method="fulltext_search">
			<result type="json" />
		</action>
		<action name="sell_item_add" class="SellItemAction">
			<result type="json" />
		</action>
		<action name="order_item_add" class="OrderItemAction">
			<result type="json" />
		</action>
		<action name="sell_item_import" class="SellItemImportAction">
			<result type="json" />
		</action>

		<action name="fee_stat" class="StatAction" method="statFee">
			<result type="json">
				<param name="excludeProperties">
					storeAmount, weekProfitList, monthProfitList
				</param>

			</result>
		</action>

	</package>

</struts>

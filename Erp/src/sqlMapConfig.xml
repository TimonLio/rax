<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="32" maxSessions="10"
		maxTransactions="5" useStatementNamespaces="true" />

	<sqlMap resource="erp/dao/ibatis/maps/Order.xml" />
	<sqlMap resource="erp/dao/ibatis/maps/OrderItem.xml" />
	<sqlMap resource="erp/dao/ibatis/maps/Sell.xml" />
	<sqlMap resource="erp/dao/ibatis/maps/SellItem.xml" />
	<sqlMap resource="erp/dao/ibatis/maps/WareCategory.xml" />
	<sqlMap resource="erp/dao/ibatis/maps/Ware.xml" />
	<sqlMap resource="erp/dao/ibatis/maps/WareGrouping.xml" />
	<sqlMap resource="erp/dao/ibatis/maps/Stat.xml" />
	
</sqlMapConfig>
